{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": { 
      "multipeer-connectivity-module": ["./multipeer-connectivity-module/src/index"],
      "multipeer-connectivity-module/*": ["./multipeer-connectivity-module/src/*"],
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
